@model IEnumerable<RRWebUI.Models.RestaurantVM>

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title></title>
</head>
<body>
    <h1>We are in Restaurant Page</h1>

    <a class="btn btn-secondary" asp-action="Create">Create Restaurant</a>

    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>Name</th>
                <th>City</th>
                <th>State</th>
                <th>Revenue</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(modelItem => item.Name)</td>
                    <td>@Html.DisplayFor(mI => item.City)</td>
                    <td>@Html.DisplayFor(modelItem => item.State)</td>
                    <td>@Html.DisplayFor(modelItem => item.Revenue)</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>